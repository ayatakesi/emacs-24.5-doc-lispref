msgstr ""

#. type: table
#: processes.texi.orig:1718
msgid "The group ID of the effective user ID, a number."
msgstr ""

#. type: item
#: processes.texi.orig:1719
#, no-wrap
msgid "group"
msgstr ""

#. type: table
#: processes.texi.orig:1721
