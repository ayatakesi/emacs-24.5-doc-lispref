msgstr ""

#. type: item
#: processes.texi.orig:1735
#, no-wrap
msgid "\"D\""
msgstr ""

#. type: table
#: processes.texi.orig:1737
msgid "uninterruptible sleep (usually I/O)"
msgstr ""

#. type: item
#: processes.texi.orig:1737
#, no-wrap
msgid "\"R\""
msgstr ""

#. type: table
#: processes.texi.orig:1739
msgid "running"
msgstr ""

#. type: item
#: processes.texi.orig:1739
#, no-wrap
msgid "\"S\""
msgstr ""

#. type: table
#: processes.texi.orig:1741
